/**
 * Demultiplexor:
 * {a, b} = {in, 0} if sel == 0
 *          {0, in} if sel == 1
 */

CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    Or(a=in,b=sel,out=aux1);
    Not(in=sel,out=aux2);
    And(a=in,b=aux1,out=b);
    And(a=aux1,b=aux2,out=a);
}
